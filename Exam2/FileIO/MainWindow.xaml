<Window x:Class="FileIO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileIO"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstSalaries" HorizontalAlignment="Left" Height="287" Margin="23,93,0,0" VerticalAlignment="Top" Width="210"/>
        <Label Content="$70,000 Salary" HorizontalAlignment="Left" Margin="85,62,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="lstEmail" HorizontalAlignment="Left" Height="287" Margin="284,93,0,0" VerticalAlignment="Top" Width="199"/>
        <Label Content="Dropbox Email" HorizontalAlignment="Left" Margin="338,62,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtAvgSalary" HorizontalAlignment="Left" Height="23" Margin="644,88,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="Average Salary:" HorizontalAlignment="Left" Margin="548,85,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnAnalyze" Content="Analyze" HorizontalAlignment="Left" Margin="605,147,0,0" VerticalAlignment="Top" Width="75" Click="BtnAnalyze_Click"/>

    </Grid>
</Window>
